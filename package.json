{
  "name": "nx-nuxt-starter",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "apps:dev": "nx run-many --target=serve",
    "web:dev": "nx run web:serve",
    "build": "npm run clean && nx run-many --target=lint,test,build --configuration=production",
    "test": "nx run-many --target=test --verbose",
    "clean": "rimraf dist",
    "lint": "nx run-many --target=lint",
    "dep-graph": "node tools/scripts/patch-graph.js && nx graph"
  },
  "devDependencies": {
    "@nrwl/cli": "15.6.3",
    "@nrwl/eslint-plugin-nx": "15.6.3",
    "@nrwl/jest": "15.6.3",
    "@nrwl/linter": "15.6.3",
    "@nrwl/node": "15.6.3",
    "@nrwl/vite": "^15.6.3",
    "@nrwl/workspace": "15.6.3",
    "@nuxt/test-utils": "^3.1.1",
    "@types/jest": "28.1.4",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "~5.49.0",
    "@typescript-eslint/parser": "~5.49.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "copyfiles": "^2.4.1",
    "eslint": "~8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "jest": "28.1.2",
    "jest-environment-jsdom": "^29.4.1",
    "jsdom": "~21.1.0",
    "nuxt": "3.1.1",
    "nx": "15.6.3",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "ts-jest": "28.0.5",
    "ts-node": "~10.9.1",
    "typescript": "^4.9.4",
    "vite-plugin-full-reload": "^1.0.5",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.25.8",
    "vue-tsc": "^1.0.24"
  }
}
